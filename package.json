{
  "name": "vacabot",
  "version": "1.0.0",
  "engines": {
    "node": "13.12.0"
  },
  "description": "Manage vacations",
  "main": "index.js",
  "dependencies": {
    "mongoose": "^5.9.7"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^7.11.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "ngrok": "^3.3.0",
    "nodemon": "^2.0.4"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "env-cmd -f .env nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schmiedeone/vacabot.git"
  },
  "keywords": [
    "slack",
    "bot",
    "vacation",
    "schmiede"
  ],
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "1000"
  },
  "author": "Schmiede One",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/schmiedeone/vacabot/issues"
  },
  "homepage": "https://github.com/schmiedeone/vacabot#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}